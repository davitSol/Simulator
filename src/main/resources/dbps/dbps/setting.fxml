<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>

<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<Tab text="설정 및 제어" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="dbps.dbps.controller.SettingController">
<content>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="605.0" prefWidth="590.0">
        <children>
            <!--통신설정 버튼-->
            <Button fx:id="communicationSettings" layoutX="53.0" layoutY="10.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="176.0" text="통신 설정" onMouseClicked="#communicationSettingClicked">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Button>
            <!--프로그램 설정-->
            <fx:include source="basicSetting.fxml"/>

            <!--고급 설정-->
            <fx:include source="advancedSetting.fxml"/>

            <!--전광판 크기-->
            <fx:include source="sizeOfDisplayBoard.fxml"/>
            <!--페이지 메시지 설정와 그 기타-->
            <fx:include source="messageSetting.fxml"/>

            <Label layoutX="298.0" layoutY="320.0" text="화면 밝기 설정" />
            <ChoiceBox layoutX="400.0" layoutY="315.0" prefHeight="25.0" prefWidth="109.0">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="100%(default)" />
                        <String fx:value="75%" />
                        <String fx:value="50%" />
                        <String fx:value="25%" />
                        <String fx:value="5%" />
                    </FXCollections>
                </items>
            </ChoiceBox>
            <Button layoutX="518.0" layoutY="315.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="50.0" text="전송" />

            <Label layoutX="292.0" layoutY="356.0" text="실시간 문구 설정" />
            <ChoiceBox layoutX="400.0" layoutY="351.0" prefHeight="25.0" prefWidth="109.0">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="효과 개별 표출" />
                        <String fx:value="효과 동시 표출" />
                    </FXCollections>
                </items>
            </ChoiceBox>
            <Button layoutX="518.0" layoutY="351.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="50.0" text="전송" />


            <!--가운데 구분선-->
            <Line endX="400.0" layoutX="250.0" layoutY="384.0" startX="-300.0" />


            <!--선아래 왼쪽-->
            <fx:include source="underTheLineLeft.fxml"/>

            <!--선아래 오른쪽-->
            <fx:include source="underTheLineRight.fxml"/>

        </children>
    </AnchorPane>
</content>
</Tab>